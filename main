    false,
    function(bool)
        _G.AutoHatch = bool
        AutoHatchChainCity()
    end
)



MiscellanousChannel:Slider("Walkspeed", 0, 300, 25, function(t)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = t
end)
